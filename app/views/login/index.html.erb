<div class= "Sign_Form">
  <h1>Log in</h1>
  <%= form_tag(:action => 'login_attempt', :controller=>"login",:method=>"GET") do %>
    <p>Username or Email:</br> <%= text_field_tag(:email) %></p> 
    <p>Password:</br> <%= password_field_tag(:encrypted_password) %></p> 
    <%= submit_tag("Log In") %>
  <% end %>
</div
